{
  "name": "theme-scheduler",
  "version": "1.0.0",
  "description": "A simple Shopify theme scheduler",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -c auto -n SERVER,client \"npm run server\" \"npm run client\"",
    "frontend": "cd frontend && npm start",
    "server": "nodemon index.js",
    "build": "cd frontend && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.1"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "busboy": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1"
  },
  "engines": {
    "npm": ">=8.0.0 <9.8.0",
    "node": "20.5.1"
  }
}
